    $(document).ready(function () {
        // Add smooth scrolling to all links
        $(document).ready(function () {
            if (window.location.hash) {
                var hash = window.location.hash.substring(1).toUpperCase();
                if (hash == 'DEPOSIT') {
                    document.getElementById("depositOpen").click();
                } if (hash == 'WITHDRAWAL') {
                    document.getElementById("withdrawalOpen").click();
                } if (hash == 'TRANSFER') {
                    document.getElementById("transferOpen").click();
                } if (hash == 'HISTORY') {
                    document.getElementById("historyOpen").click();
                    //openCity(event, 'History');
                }
                if (hash == 'RESTORE') {
                    document.getElementById("restoreOpen").click();
                }
                if (hash == 'STATEMENT') {
                    document.getElementById("statementOpen").click();
                }
                if (hash == 'REBATE') {
                    document.getElementById("restoreOpen").click();
                }
                if (hash == 'PROMOTION') {
                    document.getElementById("promotionOpen").click();
                }
            }
            else
                document.getElementById("profileOpen").click();


        });

        var $radioButtons = $('input[type="radio"]');
        $radioButtons.click(function () {
            $radioButtons.each(function () {
                $(this).parent().toggleClass('active', this.checked);
            });
        });
        $('.bank-listing input[type="radio"]').change(function () {
            if ($(this).is(':checked')) {
                $('.bank-listing li').removeClass('active');
                $(this).closest('li').addClass('active');
            }
        })

        $("#historyOpen").click(function () {
            DepositHistory();
            PromotionHistory();
            TransferHistory();
            Statement();
            RebateHistory();
            WithdrawHistory();

        });

        $("#withdrawalOpen").click(function () {
            CheckWithdrawAmountList();
        });

    });
    function myFunction() {
